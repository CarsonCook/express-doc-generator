{
  "name": "express-doc-generator",
  "version": "0.1.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "lint": "eslint \"**/*.js\" --fix"
  },
  "keywords": [],
  "author": "Carson Cook",
  "license": "MIT",
  "dependencies": {
    "glob": "^7.1.6"
  },
  "devDependencies": {
    "eslint": "^7.12.1",
    "eslint-config-google": "^0.14.0"
  },
  "expressDocGenerator": {
    "projectName": "QPlan",
    "projectDescription": "My project",
    "tag": "@express-doc-generator",
    "expressObject": "app",
    "requestObject": "req",
    "startTag": "start",
    "endTag": "end",
    "descriptionTag": "description",
    "files": "C:/Users/cookc/Desktop/Code/qplan/back-end/server/routing/*Routing.js"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es2020": true,
      "node": true,
      "mocha": true
    },
    "extends": [
      "google",
      "eslint:recommended"
    ],
    "parserOptions": {
      "ecmaVersion": 2020,
      "sourceType": "module"
    },
    "rules": {
      "require-jsdoc": 0,
      "indent": [
        "error",
        4
      ],
      "max-len": [
        "error",
        {
          "code": 170
        }
      ],
      "no-control-regex": "off"
    },
    "ignorePatterns": [
      "./**/node_modules/**/*"
    ]
  }
}
